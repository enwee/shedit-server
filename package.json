{
  "name": "snapi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "start": "node src/index.js",
    "start:dev": "nodemon ./src/index.js",
    "start:db": "mongod -dbpath ~/data/db",
    "lint": "eslint src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enwee/shedit-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.2",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "jest": "^25.1.0",
    "eslint-plugin-jest": "^23.8.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mongodb-memory-server": "^6.3.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/*.js",
      "src/routes/*.js",
      "!src/index.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    }
  }
}
